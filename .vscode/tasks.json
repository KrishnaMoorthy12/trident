{
  // See https://go.microsoft.com/fwlink/?LinkId=733558
  // for the documentation about the tasks.json format
  "version": "2.0.0",
  "tasks": [
    {
      "label": "Trident Build Bot",
      "type": "shell",
      "command": "java Build",
      "group": {
        "kind": "build",
        "isDefault": true
      }
    },
    {
      "label": "Git Send all",
      "type": "shell",
      "command": "git add . && git commit && git push"
    },
    {
      "label": "Git Sync",
      "type": "shell",
      "command": "git fetch && git pull"
    }
  ]
}
